	"scripts": {
		"start": "npm run build && node --enable-source-maps index.js",
		"build": "npm run clean && npm run swagger-gen && npm run routes-gen && tsc && npm run copy-swagger-ui",
		"test": "rm -rf dist && tsc && nyc mocha --require ts-node/register test/**/*.ts",
		"docs": "jsdoc2md --no-cache --files 'src/**/*.ts' --configure jsdoc2md.json --template 'Markdown Templates/README.hbs' > README.md && doctoc README.md",
		"clean": "rimraf dist",
		"swagger-gen": "tsoa swagger",
		"routes-gen": "tsoa routes",
		"copy-swagger-ui": "cp -r ./src/swagger-ui ./dist/swagger-ui && cp ./build/swagger.json ./dist/swagger.json",
		"tsoa": "tsoa"
	},
